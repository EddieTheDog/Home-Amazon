<!DOCTYPE html>
<html>
<head>
  <title>Admin Panel</title>
  <link rel="stylesheet" href="/style.css">
</head>
<body>
  <h1>ğŸ›  Admin Panel</h1>

  <table border="1">
    <thead>
      <tr>
        <th>ID</th>
        <th>Recipient</th>
        <th>Address</th>
        <th>Status</th>
      </tr>
    </thead>
    <tbody>
      <% packages.forEach(p => { %>
        <tr>
          <td><%= p.id %></td>
          <td><%= p.recipientName %></td>
          <td><%= p.address %></td>
          <td><%= p.status %></td>
        </tr>
      <% }) %>
    </tbody>
  </table>
</body>
</html>
